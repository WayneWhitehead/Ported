        <meta-data android:name="OnlineSearch.Key" android:value="lsSZphVTck4Z9BrwKKapcHj5M6tyD8CX" />
        <meta-data android:name="OnlineRouting.Key" android:value="lsSZphVTck4Z9BrwKKapcHj5M6tyD8CX" />
        <meta-data android:name="GeofencingApi.Key" android:value="lsSZphVTck4Z9BrwKKapcHj5M6tyD8CX" />

        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
            android:name=".intro.LoginRegister"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".intro.SplashScreen"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>